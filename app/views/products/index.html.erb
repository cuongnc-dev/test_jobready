<div class="row">
  <%= render @products %>
  <div class="col-md-3 col-sm-4 col-xs-6">
    <div class="item add-new-product">
      <%= link_to t(".add_product"), new_product_path, remote: true %>
    </div>
  </div>
</div>
<div id="my-product" class="modal fade" role="dialog">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal">&times;</button>
        <h4 class="modal-title"><%= t ".new_product_title" %></h4>
      </div>
      <div class="modal-body"></div>
    </div>
  </div>
</div>
